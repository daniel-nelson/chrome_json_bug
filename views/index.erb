<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>Demo</title>
  </head>

  <body>

    <h3>Now <a href="http://google.com">navigate elsewhere</a> and then hit the back button</h3>

  <script type="text/javascript">
function makeHttpObject() {
  try {return new XMLHttpRequest();}
  catch (error) {}
  try {return new ActiveXObject("Msxml2.XMLHTTP");}
  catch (error) {}
  try {return new ActiveXObject("Microsoft.XMLHTTP");}
  catch (error) {}

  throw new Error("Could not create HTTP request object.");
}

var request = makeHttpObject();
request.open("GET", "/docs", false);
request.setRequestHeader("Accept", "application/json");
request.send(null);
document.write('<h3 style="margin-top: 50px">XHR results</h3>');
document.write('<div>');
document.write(request.responseText.replace(/</g, '&lt;').replace(/>/g, '&gt;').replace(/\n/g, '<br />'));
document.write('</div>');
  </script>
  </body>
</html>
